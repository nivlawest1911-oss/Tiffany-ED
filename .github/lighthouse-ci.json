{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "staticDistDir": ".next"
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 1.0
                    }
                ],
                "categories:best-practices": [
                    "error",
                    {
                        "minScore": 1.0
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 1.0
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}